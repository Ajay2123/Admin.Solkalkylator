@model IEnumerable<LMS.Models.InstalledPowerMunicipality>

<h2>
   
        <button type="button" class="btn btn-default btn-lg">
            <span class="glyphicon glyphicon-plus-sign" aria-hidden="true"></span> @Html.ActionLink("Create New", "Create")
        </button>
    
</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Kommun)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BaseInstalledPower)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Inhabitants)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Kommun)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BaseInstalledPower)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Inhabitants)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ID }) |
                @Html.ActionLink("Details", "Details", new { id = item.ID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ID })
            </td>
        </tr>
    }

</table>
<div>
    <button type="button" class="btn btn-default btn-lg">
        <span class="glyphicon glyphicon-circle-arrow-left" aria-hidden="true"></span> @Html.ActionLink("Back to List", "InstalledPower", "LMS")
    </button>
</div>